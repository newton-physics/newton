{
  "version": 1,
  "project": "newton-physics",
  "project_url": "https://github.com/newton-physics/newton",
  "repo": ".",
  "branches": ["main"],
  "dvcs": "git",
  "environment_type": "virtualenv",
  "show_commit_url": "https://github.com/newton-physics/newton/commit/",
  "benchmark_dir": "asv/benchmarks",
  "env_dir": "asv/env",
  "results_dir": "asv/results",
  "html_dir": "asv/html",
  "build_cache_size": 20,
  "default_benchmark_timeout": 360,
  "build_command": ["python -m build --wheel -o {build_cache_dir} {build_dir}"],
  "install_command": [
    "python -m pip install -U numpy",
    "python -m pip install -U --pre warp-lang --index-url=https://pypi.nvidia.com/",
    "python -m pip install -U --pre mujoco -f https://py.mujoco.org/",
    "python -m pip install -U torch --index-url https://download.pytorch.org/whl/cu128",
    "python -m pip install git+https://github.com/google-deepmind/mujoco_warp.git@8e1b3b0ffb1ab6b9df3123b136bd30837fc31eab",
    "in-dir={env_dir} python -m pip install {wheel_file}[dev]"
  ]
}
